{
    "name": "tjall/laravel-mediagallery",
    "description": "A wrapper for Laravel Media Library by Spatie to add more often-used functionality.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tjall\\MediaGallery\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tjalling Feenstra",
            "email": "hello@tjalling.net"
        }
    ],
    "require": {
        "filament/support": "^3.3",
        "filament/filament": "^3.3",
        "filament/forms": "^3.3",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-package-tools": "^1.92",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-medialibrary": "^11.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tjall\\MediaGallery\\MediaGalleryServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require-dev": {
        "illuminate/support": "*",
        "tjall/laravel-modeltranslations": "@dev"
    }
}
